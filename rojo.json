{
  "name": "rdc-project",
  "servePort": 8000,
  "partitions": {
    "common": {
      "path": "src/client",
      "target": "StarterPlayer.StarterPlayerScripts.RDC"
    },
    "server": {
      "path": "src/server",
      "target": "ServerScriptService.RDC"
    },
    "client": {
      "path": "src/common",
      "target": "ReplicatedStorage.Modules.RDC"
    },
    "hot-reload": {
      "path": "src/HotReload.lua",
      "target": "ReplicatedStorage.HotReload"
    },
    "modules/roact": {
      "path": "modules/roact/lib",
      "target": "ReplicatedStorage.Modules.Roact"
    },
    "modules/rodux": {
      "path": "modules/rodux/lib",
      "target": "ReplicatedStorage.Modules.Rodux"
    },
    "modules/roact-rodux": {
      "path": "modules/roact-rodux/lib",
      "target": "ReplicatedStorage.Modules.RoactRodux"
    },
    "modules/promise": {
      "path": "modules/promise/lib",
      "target": "ReplicatedStorage.Modules.Promise"
    }
  }
}